{
  "compilerOptions": {
    "noEmit": true,
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "module": "esnext",
    "lib": ["dom", "esnext"],
    "esModuleInterop": true,
    "noImplicitAny": true,
    "outDir": "./lib/dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es6",
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": false,
    "downlevelIteration": true,
    "plugins": [{"name": "typescript-tslint-plugin"}]
  }
}
